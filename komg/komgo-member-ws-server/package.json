{
  "name": "komgo-member-ws",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --no-notify ./src/start.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'"
  },
  "author": "",
  "license": "ISC",
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "clearMocks": true
  },
  "dependencies": {
    "@komgo/error-utilities": "^5.3.3",
    "@komgo/health-check": "^2.1.0",
    "@komgo/logging": "^3.3.0",
    "@komgo/messaging-library": "^6.2.0",
    "axios": "^0.18.1",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2",
    "uuid": "^3.3.2",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.13.14",
    "@types/socket.io": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "jest": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.13.0"
  }
}
