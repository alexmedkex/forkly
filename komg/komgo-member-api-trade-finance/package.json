{
  "name": "komgo-member-api-trade-finance",
  "version": "1.0.0",
  "description": "KomGo Trade Finance API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "preinstall": "npm i -g nodemon node-inspect rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build migration:up start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:simple-debug": "node --inspect=0.0.0.0:9229 ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s swagger-gen routes-gen tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "lint": "tslint -p . -c tslint.json",
    "lint:fix": "tslint --fix -p . -c tslint.json",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}' 'integration-tests/**/*.{ts,js}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}' 'integration-tests/**/*.{ts,js}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest \"^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$\" || true' -e ts -p ./tsconfig.json",
    "test:verbose": "jest '^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$' --verbose",
    "pretest:ci": "npm run routes-gen",
    "test:ci": "jest '^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$'",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest --watchAll --verbose=false src",
    "test:watch": "npm run test:dev",
    "pretest:coverage": "npm run routes-gen",
    "test:ens": "BLOCKCHAIN_PORT=8545 jest setup-ens.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:member-setup": "BLOCKCHAIN_PORT=8545 jest members-setup.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration:lc-transitions": "INTEGRATION_TEST=true ENABLE_TEST_LOGGING=true jest LetterOfCreditTransitionService.integration.test.ts --verbose --detectOpenHandles --forceExit --silent=false",
    "test:integration:lc-nonce-increment": "INTEGRATION_TEST=true ENABLE_TEST_LOGGING=true jest LetterOfCreditNonceIncremented.integration.test.ts --verbose --detectOpenHandles --forceExit --silent=false",
    "test:lc-events": "INTEGRATION_TEST=true ENABLE_TEST_LOGGING=true jest LetterOfCreditEvents.integration.test.ts --verbose --silent=false",
    "test:partyprocessor": "ENABLE_TEST_LOGGING=true jest LetterOfCreditPartyActionProcessor.test.ts --verbose --silent=false",
    "test:lcmessaging": "INTEGRATION_TEST=true ENABLE_TEST_LOGGING=true jest LetterOfCreditMessaging.integration.test.ts  --runInBand --detectOpenHandles --forceExit --verbose --silent=false",
    "test:letterofcreditmessaging": "jest LetterOfCreditMessagingService.test.ts --runInBand --detectOpenHandles --forceExit --verbose --silent=false",
    "test:sblccreatedservice": "jest SBLCCreatedService.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:sblceventprocessor": "jest SBLCEventsProcessor.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:sblccontroller": "jest SBLCController.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:sblcservice": "jest SBLCService.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:sblcdatalayer": "jest SBLCDataAgent.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:sblctxmanager": "jest SBLCTransactionManager.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:lctxmanager": "jest LCTransactionManager.test.ts --runInBand --detectOpenHandles --forceExit --verbose=false",
    "test:integration": "USE_QUORUM_NODE=true INTEGRATION_TEST=true jest '((integration)(\\.|/)(test|spec))\\.(jsx?|tsx?)$' --detectOpenHandles --runInBand --forceExit --verbose",
    "test:integration:quorum:txmanagers": "BLOCKCHAIN_PORT=22001 USE_QUORUM_NODE=true INTEGRATION_TEST=true jest integration-tests/TransactionManagers --detectOpenHandles --forceExit --verbose",
    "test:integration:txmanagers": "INTEGRATION_TEST=true jest integration-tests/TransactionManagers --detectOpenHandles --forceExit --verbose --silent=false",
    "test:integration:consortium": "CONSORTIUM_MODE=true USE_QUORUM_NODE=true INTEGRATION_TEST=true jest '((consortium)(\\.|/)(test|spec))\\.(jsx?|tsx?)$' --detectOpenHandles --forceExit",
    "test:integration:logging": "INTEGRATION_TEST=true ENABLE_TEST_LOGGING=true npm run test:integration",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "tsc": "tsc",
    "wait-for-database": "cross-env-shell \"./config/wait-for-it.sh $MONGO_URL\"",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "globalSetup": "./integration-tests/setup/globalSetup.js",
    "globalTeardown": "./integration-tests/setup/globalTeardown.js",
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleNameMapper": {
      "@trade-finance/(.*)": "<rootDir>/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 70,
        "functions": 80,
        "lines": 80
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": true
      }
    },
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/blockchain-access": "^1.3.0",
    "@komgo/data-access": "^3.4.2",
    "@komgo/error-utilities": "^5.3.3",
    "@komgo/health-check": "^1.0.0",
    "@komgo/logging": "^3.2.0",
    "@komgo/messaging-library": "^6.2.0",
    "@komgo/microservice-config": "^7.3.1",
    "@komgo/notification-publisher": "^8.0.2",
    "@komgo/smart-contracts": "^2.9.1",
    "@komgo/types": "^24.9.1",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.7",
    "@types/node": "^10.7.1",
    "ajv": "^6.5.4",
    "amqplib": "^0.5.5",
    "async-polling": "^0.2.1",
    "axios": "^0.18.1",
    "axios-error": "^0.7.8",
    "axios-mock-adapter": "^1.16.0",
    "bcrypt-nodejs": "0.0.3",
    "bignumber.js": "^7.2.1",
    "bip39": "^2.5.0",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "eth-crypto": "^1.3.4",
    "eth-ens-namehash": "^2.0.8",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.2",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.3",
    "express-ping": "^1.4.0",
    "express-winston": "^2.6.0",
    "glob": "^7.1.2",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "jest": "^24.8.0",
    "jest-create-mock-instance": "^1.1.0",
    "js-yaml": "^3.13.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "~8.3.0",
    "lodash": "^4.17.15",
    "lz-string": "^1.4.4",
    "method-override": "^3.0.0",
    "migrate-mongo": "^6.0.1",
    "mime-types": "^2.1.12",
    "mockingoose": "^2.10.1",
    "moment": "^2.22.2",
    "mongodb": "^3.1.8",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.3.7",
    "multer": "^1.4.1",
    "ncp": "^2.0.0",
    "nock": "^10.0.6",
    "node-docker-api": "^1.1.22",
    "pako": "^1.0.6",
    "qs": "^6.5.2",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rlp": "^2.2.2",
    "solc": "^0.4.25",
    "swagger-ui-dist": "^3.19.0",
    "tsoa": "^2.3.7",
    "typed-rest-client": "^1.0.9",
    "typedi": "^0.8.0",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2",
    "wait-for-expect": "^1.2.0",
    "web3": "^1.0.0-beta.37",
    "web3-eth-abi": "^1.0.0-beta.37",
    "web3-utils": "^1.0.0-beta.37",
    "websocket": "^1.0.26",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^4.7.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "2.8.4",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/jsonwebtoken": "~7.2.8",
    "@types/lodash": "^4.14.120",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.1.4",
    "@types/morgan": "^1.7.35",
    "@types/serve-static": "^1.13.2",
    "@types/supertest": "^2.0.5",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.3.9",
    "codeclimate-test-reporter": "^0.5.0",
    "form-data": "^2.5.0",
    "istanbul": "^0.4.5",
    "jest-sonar-reporter": "^2.0.0",
    "mock-express-request": "^0.2.2",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^4.1.0",
    "prettier": "^1.14.2",
    "remap-istanbul": "^0.11.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.5.11",
    "supertest": "^3.1.0",
    "swagger-jsdoc": "^3.2.9",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0"
  }
}
