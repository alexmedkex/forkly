{
  "name": "komgo-member-api-notif",
  "version": "0.0.1",
  "description": "KomGo Notification and Task API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s swagger-gen routes-gen tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3103 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest || true' -e ts",
    "pretest:ci": "npm run routes-gen",
    "test:ci": "jest",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest --watchAll",
    "pretest:coverage": "npm run routes-gen",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:all": "CI=true npm-run-all check-formatting tslint test:coverage",
    "wait-for-database": "cross-env-shell \"./config/wait-for-it.sh $MONGO_URL\"",
    "tsc": "tsc",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false
      }
    },
    "testEnvironment": "node",
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}",
      "!src/presentation-layer/documentation/**/*",
      "!src/inversify/ioc.{ts,js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/data-access": "^3.6.0",
    "@komgo/error-utilities": "^5.3.1",
    "@komgo/health-check": "^2.1.0",
    "@komgo/logging": "^3.3.0",
    "@komgo/messaging-library": "^6.2.9",
    "@komgo/microservice-config": "^7.4.3",
    "@komgo/permissions": "^12.4.0",
    "@komgo/types": "^12.11.0",
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.5",
    "axios": "^0.18.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "class-validator": "^0.8.4",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.4",
    "express-ping": "^1.1.0",
    "glob": "^7.0.5",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "method-override": "^2.3.10",
    "migrate-mongo": "^6.0.1",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.5.15",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^2.3.8",
    "typedi": "^0.4.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/amqp-connection-manager": "^2.0.2",
    "@types/amqplib": "^0.5.9",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "0.0.29",
    "@types/jest": "^24.0.6",
    "@types/js-yaml": "^3.12.0",
    "@types/jsonwebtoken": "~7.2.0",
    "@types/method-override": "0.0.27",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.5.17",
    "@types/morgan": "^1.7.32",
    "@types/serve-static": "^1.7.27",
    "@types/supertest": "^2.0.7",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.2.0",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "jest-mock": "^24.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "mockingoose": "^2.12.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.9",
    "npm-check": "^5.7.1",
    "npm-run-all": "^3.0.0",
    "onchange": "^5.2.0",
    "prettier": "^1.16.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.4.1",
    "supertest": "^3.4.2",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0"
  }
}
