{
    "swagger": {
        "info": {
            "description": "Product management",
            "version": "1.0.0",
            "title": "Product Management"
        },
        "schemes": [ "http", "https" ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "access_token",
                "in": "header"
            }
        },
        "outputDirectory": "./dist",
        "entryFile": "./src/run-server.ts",
        "host": "localhost:4122",
        "basePath": "/v0"
    },
    "routes": {
        "basePath": "/v0",
        "authenticationModule": "./src/middleware/common/authentication/authentication.ts",
        "entryFile": "./src/run-server.ts",
        "routesDir": "./src/middleware/server-config",
        "iocModule" : "./src/inversify/ioc"
    }
}