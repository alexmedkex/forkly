{
  "name": "client",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "tsc && react-scripts test --env=jsdom",
    "test:dev": "npm run test -- --watchAll",
    "test:watch": "npm run test:dev",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:fix": "tslint -p . -c tslint.json --fix",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,tsx,js,json}'",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "test:all": "npm run tslint:ci && npm run check-formatting && CI=true npm run test:coverage",
    "analyze": "node analyzer.js",
    "eject": "react-scripts eject",
    "clear_jest": "jest --clearCache"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 66,
        "branches": 59,
        "functions": 59,
        "lines": 66
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js,tsx,jsx}",
      "!src/setupTests.ts",
      "!src/**/index.{ts,js}",
      "!src/store/initialiseStore.ts",
      "!src/fixtures/**/*.{ts,js,tsx,jsx}",
      "!src/features/notifications/fixtures/*.{ts,js,tsx,jsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{json,md}": [
        "prettier --write",
        "git add"
      ],
      "*.{ts,tsx}": [
        "prettier --write",
        "tslint",
        "git add"
      ]
    },
    "ignore": [
      "package.json",
      "package-lock.json"
    ]
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@komgo/komgo-theme": "^1.0.5",
    "@komgo/permissions": "^12.6.1",
    "@komgo/products": "^3.3.0",
    "@komgo/types": "^24.3.0",
    "@komgo/ui-components": "^1.18.1",
    "@paralleldrive/react-feature-toggles": "^2.1.0",
