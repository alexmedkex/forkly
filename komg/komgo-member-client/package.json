    "i18n-iso-countries": "^4.1.0",
    "immutable": "^3.8.2",
    "jwt-decode": "^2.2.0",
    "keycloak-js": "^4.0.0",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
