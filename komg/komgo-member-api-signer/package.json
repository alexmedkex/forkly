{
  "name": "komgo-member-api-signer",
  "version": "0.0.2",
  "description": "KomGo signer API",
  "author": "KomGo team",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:mock": "npm-run-all -s clean build:swagger-yaml copy-swagger-mock-config swagger-mock",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm run build:swagger",
    "build:swagger": "npm-run-all -s copy-swagger-config swagger-gen routes-gen && tsc && npm run copy-swagger-ui",
    "build:swagger-yaml": "npm run swagger-gen-yaml && npm run routes-gen-yaml && tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "tslint": "tslint -p . -c tslint.json ./src/**/*.ts -e ./src/middleware/routes.ts",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "rimraf ./src/middleware/server-config && mkdir ./src/middleware/server-config && tsoa routes",
    "swagger-gen-yaml": "tsoa swagger -c tsoa.yaml.json",
    "routes-gen-yaml": "tsoa routes -c tsoa.yaml.json",
    "copy-swagger-ui": "ncp ./src/presentation-layer/documentation/swagger-ui ./dist/swagger-ui",
    "copy-swagger-config": "ncp ./config-default ./config",
    "copy-swagger-mock-config": "ncp ./config-mock ./config",
    "swagger-mock": "swagger_swagger_fileName=dist/swagger.yaml swagger project edit --host 0.0.0.0 -p 81 -s & swagger_swagger_fileName=dist/swagger.yaml swagger project start -m",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest --runInBand || true' -e ts",
    "test:ci": "npm run copy-swagger-config && jest src",
    "pretest:integration": "npm run routes-gen",
    "test:integration": "jest test/integration-tests --runInBand --detectOpenHandles",
    "test:integration:dev": "jest test/integration-tests --runInBand --detectOpenHandles --watchAll",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest src/ --watchAll --runInBand",
    "pretest:coverage": "npm run routes-gen",
    "test:coverage": "npm run test:ci -- --coverage",
    "import-keys:dev": "ts-node ./src/scripts/import-keys.ts",
    "import-keys": "node ./dist/scripts/import-keys.js",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create"
  },
  "dependencies": {
    "@komgo/blockchain-access": "^3.0.0",
    "@komgo/cryptography-wrapper": "0.0.3",
    "@komgo/data-access": "^4.0.0",
    "@komgo/error-utilities": "^5.3.4",
    "@komgo/health-check": "^2.0.0",
    "@komgo/jose": "^5.1.1",
    "@komgo/logging": "^3.2.0",
    "@komgo/messaging-library": "^6.2.0",
    "@komgo/microservice-config": "^7.3.1",
    "@komgo/types": "^24.6.0",
    "async-polling": "^0.2.1",
    "axios": "^0.18.1",
    "class-validator": "^0.9.1",
    "config": "^1.21.0",
    "eth-crypto": "^1.3.4",
    "ethers": "^3.0.27",
    "express": "^4.14.0",
    "express-ping": "^1.1.0",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "method-override": "^2.3.10",
    "migrate-mongo": "^6.0.1",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.1",
    "ncp": "^2.0.0",
    "npm": "^6.10.1",
    "npm-run-all": "^3.1.2",
    "reflect-metadata": "^0.1.3",
    "tsoa": "^2.3.81",
    "typedarray-to-buffer": "^3.1.5",
    "typescript": "^3.3.3",
    "wait-for-expect": "^1.1.0",
    "web3": "1.0.0-beta.37",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^5.1.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "0.0.29",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.0",
    "@types/js-yaml": "^3.12.0",
    "@types/jsonwebtoken": "~7.2.0",
    "@types/method-override": "0.0.27",
    "@types/mongoose": "^5.3.17",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.10.49",
    "@types/serve-static": "^1.7.27",
    "@types/supertest": "^2.0.7",
    "@types/swagger-express-mw": "0.7.6",
    "@types/web3": "^1.0.18",
    "@types/winston": "^2.4.4",
    "amqplib": "^0.5.2",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "node-docker-api": "^1.1.22",
    "nodemon": "^1.18.11",
    "npm-check": "^5.7.1",
    "onchange": "^5.2.0",
    "prettier": "^1.13.5",
    "promievent": "0.0.1",
    "remap-istanbul": "^0.12.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.1",
    "swagger-jsdoc": "^3.2.9",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.13.0"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 84,
        "branches": 62,
        "functions": 89,
        "lines": 85
      }
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "babelConfig": false
      }
    },
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}",
      "!src/presentation-layer/documentation/**/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true
}
