{
  "name": "komgo-api-onboarding",
  "version": "0.0.1",
  "description": "KomGo API with Express, node 8, mongo, and JWT",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:mock": "npm-run-all -s clean build:swagger-yaml",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s swagger-gen routes-gen tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3103 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "prettier": "prettier --write '{src,integration-tests}/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different '{src,integration-tests}/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "jest src",
    "test:dev": "jest src --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "test:all": "CI=true npm-run-all check-formatting tslint test:coverage",
    "tsc": "tsc",
    "test:integration": "jest integration-tests --verbose --runInBand --bail 1 --detectOpenHandles",
    "test:integration:dev": "jest integration-tests --verbose --runInBand --bail --detectOpenHandles --watchAll",
    "wait-for-database": "cross-env-shell \"./config/wait-for-it.sh $MONGO_URL\"",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create",
    "transfer-ownership-to-komgo-onboarder": "node ./dist/scripts/transfer-node-ownership.js"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 75,
        "functions": 94,
        "lines": 95
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": false
      }
    },
    "preset": "ts-jest",
    "testMatch": null,
    "testEnvironment": "node",
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}",
      "!src/presentation-layer/documentation/**/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/blockchain-access": "^4.1.1",
    "@komgo/data-access": "^3.6.0",
    "@komgo/error-utilities": "^5.3.3",
    "@komgo/health-check": "^2.1.0",
    "@komgo/logging": "^3.3.0",
    "@komgo/messaging-library": "^6.2.0",
    "@komgo/microservice-config": "^7.5.1",
    "@komgo/notification-publisher": "^8.0.3",
    "@komgo/products": "^3.3.0",
    "@komgo/types": "^19.3.1",
    "axios": "^0.18.1",
    "bn.js": "^4.11.8",
    "bn.js-typings": "^1.0.1",
    "body-parser": "^1.19.0",
    "camelcase": "^5.3.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "email-validator": "^2.0.4",
    "eth-ens-namehash": "^2.0.8",
    "express": "^4.17.0",
    "express-ping": "^1.1.0",
    "express-winston": "^2.6.0",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "jest-mock": "^24.8.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "migrate-mongo": "^6.0.1",
    "mongodb": "^3.2.7",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.5.15",
    "node-fetch": "^2.2.0",
    "pako": "^1.0.6",
    "reflect-metadata": "^0.1.3",
    "truffle-contract": "^4.0.17",
    "tsoa": "^2.3.81",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.37",
    "web3-utils": "1.0.0-beta.41",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.30",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.135",
    "@types/method-override": "0.0.27",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/node": "^11.9.5",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.2.0",
    "axios-mock-adapter": "^1.16.0",
    "cross-env": "^5.2.0",
    "human-readable-ids": "^1.0.4",
    "jest": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "mock-http-server": "^1.2.0",
    "mockingoose": "^2.13.1",
    "moxios": "^0.4.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.19.0",
    "npm-check": "^5.7.1",
    "npm-run-all": "^3.0.0",
    "onchange": "^5.2.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.1",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^6.0.5",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.14.0",
    "wait-for-expect": "^1.2.0"
  }
}
