{
  "name": "komgo-member-api-coverage",
  "version": "1.0.0",
  "description": "KomGo Coverage API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "preinstall": "npm i -g nodemon node-inspect rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build migration:up start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:simple-dev": "node --inspect=0.0.0.0:9229 ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean migration:up build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s swagger-gen routes-gen tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "lint": "tslint -p . -c tslint.json",
    "lint:fix": "tslint --fix -p . -c tslint.json",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "prettier": "prettier --write '{src,integration-tests}/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different '{src,integration-tests}/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest src --verbose || true' -e ts -p ./tsconfig.json",
    "pretest:ci": "npm run routes-gen",
    "test:ci": "jest src",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest --watchAll",
    "test:watch": "npm run test:dev",
    "pretest:coverage": "npm run routes-gen",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration": "jest integration-tests --runInBand --detectOpenHandles --forceExit",
    "test:integration:debug": "node  --inspect=0.0.0.0:9229 ./node_modules/.bin/jest  integration-tests --runInBand --detectOpenHandles --forceExit ",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "tsc": "tsc",
    "wait-for-database": "cross-env-shell \"./config/wait-for-it.sh $MONGO_URL\"",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 97,
        "branches": 90,
        "functions": 96,
        "lines": 97
      }
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "babelConfig": false
      }
    },
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/data-access": "^4.0.0",
    "@komgo/error-utilities": "^5.3.1",
    "@komgo/health-check": "^2.0.0",
    "@komgo/integration-test-utilities": "^5.1.0",
    "@komgo/logging": "^3.2.0",
    "@komgo/messaging-library": "^6.2.9",
    "@komgo/microservice-config": "^7.3.1",
    "@komgo/notification-publisher": "^7.1.1",
    "@komgo/types": "^24.6.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.15",
    "axios": "^0.18.1",
    "axios-error": "^0.7.11",
    "body-parser": "^1.18.3",
    "camelcase": "^5.0.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.4",
    "express-ping": "^1.4.0",
    "express-winston": "^2.6.0",
    "glob": "^7.1.2",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "~8.3.0",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "migrate-mongo": "^6.0.1",
    "moment": "^2.23.0",
    "mongodb": "^3.1.10",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.1",
    "ncp": "^2.0.0",
    "query-string": "^6.2.0",
    "reflect-metadata": "^0.1.12",
    "tsoa": "^2.4.0",
    "typedi": "^0.8.0",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2",
    "websocket": "^1.0.28",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "2.8.4",
    "@types/jest": "^23.3.10",
    "@types/js-yaml": "^3.12.0",
    "@types/jsonwebtoken": "~7.2.8",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.1.17",
    "@types/supertest": "^2.0.5",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.3.9",
    "axios-mock-adapter": "^1.15.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "mock-express-request": "^0.2.2",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.5.8",
    "supertest": "^3.1.0",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.14.0",
    "wait-for-expect": "^1.0.1",
    "web3": "^1.0.0-beta.37"
  }
}
