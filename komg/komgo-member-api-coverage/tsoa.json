{
    "swagger": {
        "info": {
            "description": "Coverage service",
            "version": "0.0.0",
            "title": "Coverage service"
        },
        "tags":[
        ],
        "schemes": [ "http", "https" ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "access_token",
                "in": "header"
            }
        },
        "outputDirectory": "./dist",
        "entryFile": "./src/run-server.ts",
        "host": "localhost:3110",
        "basePath": "/v0",
        "yaml": false
    },
    "routes": {
        "basePath": "/v0",
        "authenticationModule": "./src/middleware/authentication.ts",
        "entryFile": "./src/run-server.ts",
        "routesDir": "./src/middleware/server-config",
        "iocModule" : "./src/inversify/ioc"
    }
}



