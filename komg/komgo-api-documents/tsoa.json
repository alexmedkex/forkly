{
    "swagger": {
        "info": {
            "description": "A Login authorization and registration using jwt for adding movies",
            "version": "0.0.1",
            "title": "User Login Auth and Registration for adding movies"
        },
        "tags": [
            {
                "name": "auth",
                "description": "Access to updating a secured data"
            },
            {
                "name": "user",
                "description": "Operations about user"
            },
            {
                "name": "role",
                "description": "Operations about roles"
            }
        ],
        "schemes": [
            "http",
            "https"
        ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "access_token",
                "in": "header"
            }
        },
        "outputDirectory": "./dist",
        "entryFile": "./src/run-server.ts",
        "host": "localhost:3106",
        "basePath": "/v0",
        "specMerging": "recursive",
        "spec": {
            "paths": {
                "/trade-finance/categories/{categoryId}/types/{typeId}/upload": {
                    "post": {
                        "consumes": [
                            "multipart/form-data"
                        ],
                        "parameters": [
                            {
                                "in": "formData",
                                "name": "fileData",
                                "required": true,
                                "type": "file"
                            },
                            {
                                "in": "formData",
                                "name": "extraData",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "categoryId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "typeId",
                                "required": true,
                                "type": "string"
                            }
                        ]
                    }
                },
                "/products/{productId}/categories/{categoryId}/types/{typeId}/documents": {
                    "post": {
                        "consumes": [
                            "multipart/form-data"
                        ],
                        "parameters": [
                            {
                                "in": "formData",
                                "name": "fileData",
                                "required": true,
                                "type": "file"
                            },
                            {
                                "in": "formData",
                                "name": "extraData",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "productId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "categoryId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "typeId",
                                "required": true,
                                "type": "string"
                            }
                        ]
                    }
                },
                "/products/{productId}/categories/{categoryId}/types/{typeId}/upload": {
                    "post": {
                        "consumes": [
                            "multipart/form-data"
                        ],
                        "parameters": [
                            {
                                "in": "formData",
                                "name": "fileData",
                                "required": true,
                                "type": "file"
                            },
                            {
                                "in": "formData",
                                "name": "extraData",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "productId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "categoryId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "path",
                                "name": "typeId",
                                "required": true,
                                "type": "string"
                            },
                            {
                                "in": "header",
                                "name": "jwt",
                                "required": false,
                                "type": "string"
                            }
                        ]
                    }
                },
                "/products/{productId}/documents/{documentId}/content": {
                    "get": {
                        "responses": {
                            "200": {
                                "description": "Ok",
                                "schema": {
                                    "type": "file",
                                    "format": "binary"
                                }
                            }
                        }
                    }
                },
                "/products/{productId}/ext-shared-documents/{documentId}/content": {
                    "get": {
                        "responses": {
                            "200": {
                                "description": "Ok",
                                "schema": {
                                    "type": "file",
                                    "format": "binary"
                                }
                            }
                        }
                    }
                },
                "/document-templates": {
                    "post": {
                        "consumes": [
                            "multipart/form-data"
                        ],
                        "parameters": [
                            {
                                "in": "formData",
                                "name": "fileData",
                                "required": true,
                                "type": "file",
                                "description": "Template to be uploaded"
                            }
                        ]
                    }
                },
                "/document-templates/generate-document": {
                    "post": {
                        "responses": {
                            "200": {
                                "description": "Ok",
                                "schema": {
                                    "type": "file",
                                    "format": "binary"
                                }
                            }
                        }
                    }
                },
                "/trade-finance/documents/{documentId}/content": {
                    "get": {
                        "responses": {
                            "200": {
                                "description": "Ok",
                                "schema": {
                                    "type": "file",
                                    "format": "binary"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "routes": {
        "basePath": "/v0",
        "entryFile": "./src/run-server.ts",
        "routesDir": "./src/middleware/server-config",
        "authenticationModule": "./src/business-layer/security/Authentication",
        "iocModule": "./src/inversify/ioc"
    },
    "ignore": [
        "**/node_modules/@komgo/api-blockchain-signer-client/dist/**"
    ]
}
