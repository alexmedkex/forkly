{
  "name": "komgo-member-api-documents",
  "version": "0.0.1",
  "description": "KomGo documents API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:mock": "npm-run-all -s clean build:swagger-yaml copy-swagger-mock-config swagger-mock",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm run copy-swagger-config && npm run swagger-gen && npm run routes-gen && tsc && npm run copy-swagger-ui",
    "build:swagger-yaml": "npm run swagger-gen-yaml && npm run routes-gen-yaml && tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "lint": "tslint -p . -c tslint.json",
    "lint:fix": "tslint --fix -p . -c tslint.json",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "swagger-gen-yaml": "tsoa swagger -c tsoa.yaml.json",
    "routes-gen-yaml": "tsoa routes -c tsoa.yaml.json",
    "copy-swagger-ui": "ncp ./src/presentation-layer/documentation/swagger-ui ./dist/swagger-ui",
    "copy-swagger-config": "ncp ./config-default ./config",
    "copy-swagger-mock-config": "ncp ./config-mock ./config",
    "swagger-mock": "swagger_swagger_fileName=dist/swagger.yaml swagger project edit --host 0.0.0.0 -p 81 -s & swagger_swagger_fileName=dist/swagger.yaml swagger project start -m",
    "prettier": "prettier --write '{src,test}/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different '{src,test}/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest src || true' -e ts",
    "pretest:ci": "npm run routes-gen",
    "test:ci": "npm run copy-swagger-config && jest src",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest src --watchAll",
    "pretest:coverage": "npm run routes-gen",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration": "INTEGRATION_TESTS=true jest test/integration-tests --verbose",
    "test:integration:dev": "LOCAL_EXECUTION=true INTEGRATION_TESTS=true jest /integration-tests --watchAll",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "wait-for-database": "cross-env-shell \"./config/wait-for-it.sh $MONGO_URL\"",
    "migration:up": "migrate-mongo -f config/db-migrations.js up",
    "migration:down": "migrate-mongo -f config/db-migrations.js down",
    "migration:create": "migrate-mongo -f config/db-migrations.js create",
    "add-docs-to-registry-v2": "node ./dist/scripts/add-docs-to-registry-v2.js"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 93,
        "branches": 81,
        "functions": 89,
        "lines": 93
      }
    },
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "babelConfig": false
      }
    },
    "globalTeardown": "<rootDir>/test/integration-tests/teardown.js",
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.ts",
      "!src/**/{routes,index}.ts",
      "!src/presentation-layer/documentation/**/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "KOMGO API Documents test report",
          "outputPath": "./test-reports/test-execution-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "theme": "lightTheme",
          "executionTimeWarningThreshold": 5,
          "dateFormat": "yyyy-mm-dd HH:MM:ss",
          "sort": "default"
        }
      ]
    ]
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/api-blockchain-signer-client": "^2.0.1",
    "@komgo/blockchain-access": "^3.0.0",
    "@komgo/data-access": "^4.0.0",
    "@komgo/document-generator": "^1.5.1",
    "@komgo/error-utilities": "^5.3.1",
    "@komgo/health-check": "^2.0.0",
    "@komgo/logging": "^3.2.1",
    "@komgo/messaging-library": "^6.2.0",
    "@komgo/messaging-types": "^2.3.0",
    "@komgo/microservice-config": "^7.4.1",
    "@komgo/notification-publisher": "^7.1.2",
    "@komgo/retry": "^2.2.1",
    "@komgo/types": "^24.6.0",
    "axios": "^0.18.1",
    "axios-error": "^0.7.6",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.15.2",
    "bson": "^1.1.0",
    "class-transformer": "^0.1.9",
    "class-transformer-validator": "^0.6.0",
    "class-validator": "^0.9.1",
    "config": "^1.21.0",
    "cookie-parser": "^1.4.3",
    "crypto": "^1.0.1",
    "eth-ens-namehash": "^2.0.8",
    "ethereumjs-util": "^5.2.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.14.0",
    "express-ping": "^1.1.0",
    "glob": "^7.0.5",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "hummus-recipe": "^1.8.5",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "merkle-tree-solidity": "^1.0.8",
    "method-override": "^2.3.10",
    "migrate-mongo": "^6.0.1",
    "moment": "^2.24.0",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.7.1",
    "mongoose-gridfs": "^0.5.0",
    "multer": "^1.3.1",
    "npm": "^6.1.0",
    "reflect-metadata": "^0.1.3",
    "tsoa": "^2.3.7",
    "typed-rest-client": "^1.0.7",
    "typedarray-to-buffer": "^3.1.5",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^5.1.0",
    "@types/bson": "^4.0.0",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.0.30",
    "@types/jest": "^23.1.0",
    "@types/js-yaml": "^3.12.0",
    "@types/jsonwebtoken": "~7.2.0",
    "@types/lodash": "^4.14.116",
    "@types/method-override": "0.0.27",
    "@types/mongodb": "3.1.19",
    "@types/mongoose": "^5.3.26",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.18",
    "@types/serve-static": "^1.7.27",
    "@types/swagger-express-mw": "0.7.6",
    "axios-mock-adapter": "^1.15.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "form-data": "^2.3.3",
    "get-port": "^4.1.0",
    "jest": "^24.8.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporter": "^2.4.3",
    "jest-mock": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "mock-http-server": "^1.0.0",
    "mockingoose": "^2.10.1",
    "mockserver-client": "^5.5.1",
    "mongodb": "^3.1.8",
    "ncp": "^2.0.0",
    "node-docker-api": "^1.1.22",
    "node-mongodb-fixtures": "^2.3.3",
    "nodemon": "^1.11.0",
    "npm-check": "^5.7.1",
    "npm-run-all": "^3.1.2",
    "onchange": "^6.0.0",
    "prettier": "^1.13.5",
    "remap-istanbul": "^0.12.0",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.1",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1",
    "wait-for-expect": "^1.2.0",
    "web3-utils": "1.0.0-beta.37"
  }
}
