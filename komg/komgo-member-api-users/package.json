{
  "name": "komgo-member-api-users",
  "version": "1.0.0",
  "description": "KomGo users API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s copy-swagger-config swagger-gen routes-gen tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "tslint": "tslint -p . -c tslint.json",
    "tslint:ci": "npm run tslint -- -t json -o report-lint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-config": "ncp ./config-default ./config",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "tsc": "tsc",
    "test": "npm run copy-swagger-config && jest '^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$'",
    "test:dev": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "test:integration": "jest '((integration)(\\.|/)(test|spec))\\.(jsx?|tsx?)$' --runInBand --detectOpenHandles --forceExit --verbose",
    "test:all": "CI=true npm-run-all check-formatting tslint test:coverage",
    "pretest:dev": "npm run routes-gen",
    "pretest:coverage": "npm run routes-gen"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 87,
        "functions": 95,
        "lines": 95
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false
      }
    },
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}",
      "!src/presentation-layer/documentation/**/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "preset": "ts-jest",
    "testMatch": null,
    "testEnvironment": "node"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/data-access": "^3.8.0",
    "@komgo/error-utilities": "^5.3.1",
    "@komgo/health-check": "^2.0.1",
    "@komgo/keycloak-admin": "^1.13.5",
    "@komgo/logging": "^3.2.1",
    "@komgo/microservice-config": "^7.13.0",
    "@komgo/types": "^24.5.0",
    "@types/mongodb": "^3.1.28",
    "axios": "^0.18.1",
    "axios-mock-adapter": "^1.15.0",
    "class-validator": "^0.8.4",
    "config": "^1.21.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.4",
    "express-winston": "^2.6.0",
    "glob": "^7.0.5",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "mockingoose": "^2.13.1",
    "mongodb": "^3.2.7",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^5.5.15",
    "ncp": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "tsoa": "2.5.1",
    "typedi": "^0.4.1",
    "typescript": "^3.2.2",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.15.2",
    "@types/config": "0.0.29",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/js-yaml": "^3.5.25-alpha",
    "@types/jsonwebtoken": "~7.2.0",
    "@types/lodash": "^4.14.116",
    "@types/method-override": "0.0.27",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.10.25",
    "@types/serve-static": "^1.7.27",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.2.0",
    "jest": "^24.8.0",
    "jest-mock": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.18.3",
    "npm-check": "^5.9.0",
    "npm-run-all": "^3.0.0",
    "onchange": "^5.2.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.4.1",
    "swagger-jsdoc": "^3.2.9",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0"
  }
}
