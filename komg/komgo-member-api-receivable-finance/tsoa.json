{
  "swagger": {
    "info": {
      "description": "Service used to create receivables discounting data",
      "version": "0.0.1",
      "title": "Service used to create receivables discounting data"
    },
    "tags": [],
    "schemes": ["http", "https"],
    "securityDefinitions": {
