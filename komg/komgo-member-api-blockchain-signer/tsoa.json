{
    "swagger": {
        "info": {
            "description": "Endpoint to sign, verify, encrypt and decrypt messages",
            "version": "0.0.1",
            "title": "Endpoint to sign, verify, encrypt and decrypt messages"
        },
        "tags":[
        ],
        "schemes": [ "http", "https" ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
