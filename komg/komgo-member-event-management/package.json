{
  "name": "komgo-member-event-management",
  "version": "1.0.1",
  "description": "Komgo micro-service that listens all relevant incoming messages coming from either the Intra-Platform MQ Layer or the Internal MQ Layer",
  "author": "KomGo team",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm-run-all -s copy-configs run-tsc",
    "start": "npm-run-all -s clean build start:simple",
    "start:dev": "npm-run-all clean build -p watch:*",
    "start:simple": "node ./dist/main.js",
    "clean:js": "del-cli test/**/*.js",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "lint": "tslint -p . -c tslint.json",
    "run-tsc": "./node_modules/.bin/tsc",
    "copy-configs": "ncp ./config-default ./config",
    "prettier": "prettier --write '{src,test}/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different '{src,test}/**/*.{ts,ts,js,json}'",
    "watch:node": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/main.js -e ts",
    "watch:ts": "./node_modules/.bin/tsc --watch",
    "test": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 --exec 'jest --runInBand || true' -e ts",
    "test:dev": "jest --watchAll --runInBand",
    "test:ci": "jest src",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration": "jest test/integration-tests/*.integration.test.ts --runInBand --detectOpenHandles",
    "test:integration:ci": "jest test/integration-tests --runInBand --detectOpenHandles --coverage",
    "test:integration:dev": "jest test/integration-tests/*.integration.test.ts --runInBand --detectOpenHandles --watchAll",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "is-ready": "node ./dist/is-ready.js",
    "is-ready:dev": "ts-node ./src/is-ready.ts"
  },
  "dependencies": {
    "@komgo/data-access": "^4.0.0",
    "@komgo/error-utilities": "^5.3.3",
    "@komgo/health-check": "^2.1.0",
    "@komgo/logging": "^3.3.0",
    "@komgo/messaging-library": "^6.2.9",
    "@komgo/types": "^24.9.0",
    "@types/node": "^10.14.18",
    "JSONStream": "^1.3.5",
    "async-polling": "^0.2.1",
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "bottleneck": "^2.18.1",
    "config": "^2.0.2",
    "eth-ens-namehash": "^2.0.8",
    "event-stream": "3.3.5",
    "express": "^4.16.4",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^4.0.0",
    "js-yaml": "^3.13.1",
    "mongodb": "^3.2.4",
    "mongoose": "^5.7.1",
    "ncp": "^2.0.0",
    "node-docker-api": "^1.1.22",
    "reflect-metadata": "^0.1.3",
    "uuid": "^3.3.2",
    "wait-for-expect": "^1.1.0",
    "websocket": "^1.0.26",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^5.1.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/js-yaml": "^3.12.1",
    "@types/mongodb": "^3.1.26",
    "amqplib": "^0.5.3",
    "axios-mock-adapter": "^1.16.0",
    "del-cli": "^1.1.0",
    "jest": "^24.7.1",
    "jest-create-mock-instance": "^1.1.0",
    "jest-mock": "^23.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5",
    "validator": "^10.11.0"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 84,
        "branches": 75,
        "functions": 78,
        "lines": 83
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": false
      }
    },
    "preset": "ts-jest",
    "testMatch": null,
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "messaging/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "keywords": [
    "typescript",
    "docker"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true
}
