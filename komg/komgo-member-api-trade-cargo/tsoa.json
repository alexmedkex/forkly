{
  "swagger": {
    "schemes": [
      "http",
      "https"
    ],
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "access_token",
        "in": "header"
      }
    },
    "outputDirectory": "./dist",
    "entryFile": "./src/run-server.ts",
    "host": "localhost:3108",
    "basePath": "/v0",
    "yaml": false,
    "specMerging": "recursive",
    "spec": {
      "definitions": {
        "ICargo": {
          "allOf": [
            {
              "$ref": "#/definitions/ICargoBase"
            },
            {
              "type": "object",
              "properties": {
                "createdAt": {
                  "type": "string"
                },
                "updatedAt": {
                  "type": "string"
                },
                "_id": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "deletedAt": {
                  "type": "string",
                  "x-nullable": true
                }
              },
              "required": [
                "source",
                "sourceId",
                "grade",
                "parcels",
                "cargoId",
                "createdAt",
                "updatedAt",
                "_id",
                "status"
              ]
            }
          ]
        }
      },
      "paths": {}
    }
  },
  "routes": {
    "basePath": "/v0",
    "entryFile": "./src/run-server.ts",
    "routesDir": "./src",
    "authenticationModule": "./src/middleware/authentication.ts",
    "iocModule": "./src/inversify/ioc",
    "middlewareTemplate": "./src/tsoa-template.hbs"
  }
}