{
  "name": "komgo-member-blockchain-event-management",
  "version": "2.0.0",
  "description": "Komgo micro-service that listens all relevant blockchain events",
  "author": "KomGo team",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm-run-all -s copy-configs run-tsc",
    "start": "npm-run-all -s clean build start:simple",
    "start:dev": "npm-run-all clean build -p watch:*",
    "start:simple": "node ./dist/main.js",
    "clean:js": "del-cli test/**/*.js",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "lint": "tslint -p . -c tslint.json",
    "run-tsc": "./node_modules/.bin/tsc",
    "copy-configs": "ncp ./config-default ./config",
    "prettier": "prettier --write '{src,test}/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different '{src,test}/**/*.{ts,ts,js,json}'",
    "watch:node": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/main.js -e ts",
    "watch:ts": "./node_modules/.bin/tsc --watch",
    "test:dev": "jest src --watchAll --runInBand",
    "test:ci": "jest src",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration": "jest test/integration-tests --runInBand --detectOpenHandles --forceExit",
    "test:integration:coverage": "jest integration-tests --runInBand --detectOpenHandles --coverage --forceExit",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "is-ready": "node ./dist/is-ready.js",
    "is-ready:dev": "ts-node ./src/is-ready.ts"
  },
  "dependencies": {
    "@komgo/blockchain-access": "^3.0.0",
    "@komgo/data-access": "^4.0.0",
    "@komgo/error-utilities": "^5.3.3",
    "@komgo/health-check": "^2.0.3",
    "@komgo/logging": "^3.2.1",
    "@komgo/messaging-library": "^6.2.4",
    "@komgo/smart-contracts": "2.9.1",
    "@komgo/types": "^24.9.0",
    "JSONStream": "^1.3.5",
    "async-polling": "^0.2.1",
    "axios": "^0.19.0",
    "base-64": "^0.1.0",
    "bottleneck": "^2.18.0",
    "config": "^2.0.2",
    "eth-ens-namehash": "^2.0.8",
    "event-stream": "3.3.5",
    "express": "^4.17.1",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^4.0.0",
    "js-yaml": "^3.13.1",
    "mongodb": "^3.2.3",
    "mongoose": "^5.7.1",
    "ncp": "^2.0.0",
    "node-docker-api": "^1.1.22",
    "reflect-metadata": "^0.1.3",
    "uuid": "^3.3.2",
    "wait-for-expect": "^1.1.0",
    "web3": "1.0.0-beta.37",
    "web3-utils": "1.0.0-beta.37",
    "websocket": "^1.0.26",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^5.1.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mongodb": "^3.1.24",
    "@types/mongoose": "^5.3.26",
    "@types/node": "^11.13.4",
    "amqplib": "^0.5.3",
    "axios-mock-adapter": "^1.16.0",
    "del-cli": "^1.1.0",
    "jest": "^24.7.1",
    "jest-create-mock-instance": "^1.1.0",
    "jest-mock": "^24.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-helpers": "^1.1.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4",
    "validator": "^10.11.0",
    "web3-core": "1.0.0-beta.37",
    "web3-eth-contract": "1.0.0-beta.37"
  },
  "keywords": [
    "typescript",
    "docker"
  ],
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 85,
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": false
      }
    },
    "preset": "ts-jest",
    "testMatch": null,
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/*/**/ioc.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "testEnvironment": "node"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true
}
