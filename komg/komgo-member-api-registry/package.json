{
  "name": "komgo-member-api-registry",
  "version": "1.0.0",
  "description": "KomGo Registry API",
  "author": "KomGo team",
  "license": "MIT",
  "main": "dist/run-server.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "preinstall": "npm i -g nodemon node-inspect rimraf npm-run-all npm-check",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node ./dist/run-server.js",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:mock": "npm-run-all -s clean build:swagger-yaml swagger-mock",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:swagger",
    "build:swagger": "npm-run-all -s swagger-gen routes-gen tsc",
    "build:swagger-yaml": "npm-run-all swagger-gen-yaml routes-gen-yaml tsc",
    "watch:tsoa": "onchange './src/**/*.*' -d 3000 -- npm run swagger-gen && npm run routes-gen",
    "watch:node": "nodemon --inspect=0.0.0.0:9229 --watch src --delay 2.5 ./dist/run-server.js -e ts",
    "watch:ts": "tsc --watch",
    "lint": "tslint -p . -c tslint.json",
    "lint:fix": "tslint --fix -p . -c tslint.json",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "swagger-gen-yaml": "tsoa swagger -c tsoa.yaml.json",
    "routes-gen-yaml": "tsoa routes -c tsoa.yaml.json",
    "swagger-mock": "swagger_swagger_fileName=dist/swagger.yaml swagger project edit --host 0.0.0.0 -p 81 -s & swagger_swagger_fileName=dist/swagger.yaml swagger project start -m",
    "prettier": "prettier --write 'src/**/*.{ts,js,json}'",
    "check-formatting": "prettier --list-different 'src/**/*.{ts,ts,js,json}'",
    "tsoa": "tsoa",
    "pretest": "npm run routes-gen",
    "test": "nodemon --inspect=0.0.0.0:9229 --exec 'jest \"^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$\" || true' -e ts -p ./tsconfig.json",
    "pretest:ci": "npm run routes-gen",
    "test:ci": "jest '^(((?!integration.test.(tsx?|jsx?)).)*(test|spec)\\.(tsx?|jsx?))$'",
    "pretest:dev": "npm run routes-gen",
    "test:dev": "jest --watchAll",
    "pretest:coverage": "npm run routes-gen",
    "test:coverage": "npm run test:ci -- --coverage",
    "test:integration": "jest '((integration)(\\.|/)(test|spec))\\.(jsx?|tsx?)$' --runInBand --detectOpenHandles --forceExit --verbose",
    "ci": "CI=true npm-run-all check-formatting lint test:ci",
    "tsc": "tsc"
  },
  "jestSonar": {
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "jest": {
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "coverageThreshold": {
      "global": {
        "statements": 87,
        "branches": 81,
        "functions": 82,
        "lines": 87
      }
    },
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": false
      }
    },
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/*/**/*.{ts,js}",
      "!src/**/{routes,index}.{ts,js}",
      "!src/presentation-layer/documentation/**/*",
      "!src/integration-tests/**/*"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~8.11.3"
  },
  "engineStrict": true,
  "dependencies": {
    "@komgo/blockchain-access": "^4.2.0",
    "@komgo/data-access": "^3.7.0",
    "@komgo/error-utilities": "^5.3.1",
    "@komgo/health-check": "^2.0.0",
    "@komgo/jose": "^5.0.0",
    "@komgo/logging": "^3.2.1",
    "@komgo/messaging-library": "^6.1.0",
    "@komgo/microservice-config": "^7.11.0",
    "@komgo/smart-contracts": "^2.2.0",
    "@komgo/types": "^24.7.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.7.1",
    "amqplib": "^0.5.3",
    "async-polling": "^0.2.1",
    "axios": "^0.18.1",
    "bcrypt-nodejs": "0.0.3",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "config": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "eth-ens-namehash": "^2.0.8",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.3",
    "express-ping": "^1.4.0",
    "glob": "^7.1.2",
    "inversify": "^4.13.0",
    "inversify-binding-decorators": "^3.2.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "~8.3.0",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.3",
    "mongoose": "^5.3.6",
    "ncp": "^2.0.0",
    "pako": "^1.0.6",
    "reflect-metadata": "^0.1.12",
    "truffle-contract": "^4.0.19",
    "tsoa": "^2.3.81",
    "typedi": "^0.8.0",
    "typescript": "^3.0.1",
    "web3": "1.0.0-beta.37",
    "websocket": "^1.0.26",
    "winston": "^2.2.4"
  },
  "devDependencies": {
    "@komgo/integration-test-utilities": "^3.2.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "1.17.0",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "2.8.4",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/jsonwebtoken": "~7.2.8",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.1.4",
    "@types/morgan": "^1.7.35",
    "@types/serve-static": "^1.13.2",
    "@types/supertest": "^2.0.5",
    "@types/swagger-express-mw": "0.7.6",
    "@types/winston": "^2.3.9",
    "axios-mock-adapter": "^1.16.0",
    "codeclimate-test-reporter": "^0.5.0",
    "istanbul": "^0.4.5",
    "jest": "^24.1.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "mockingoose": "^2.10.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.2.0",
    "prettier": "^1.14.2",
    "remap-istanbul": "^0.11.1",
    "rimraf": "^2.5.2",
    "source-map-support": "^0.5.8",
    "supertest": "^3.1.0",
    "swagger-jsdoc": "^3.2.9",
    "ts-helpers": "^1.1.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0"
  }
}
